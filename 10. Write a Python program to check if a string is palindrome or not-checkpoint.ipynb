    "a=input(\"Enter a name \")\n",
    "i=0\n",
    "l=len(a)\n",
    "while(l>i):\n",
    "    l=l-1\n",
    "    if(a[i]!=a[l]):\n",
    "        print(\"Not a palindrome\")\n",
    "        break\n",
    "    else:\n",
    "        i=i+1\n",
    "    print(\"Palindrome\")\n",
    "    break"
